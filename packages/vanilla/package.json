{
  "name": "@m-fe/whiteboard",
  "version": "0.0.3",
  "main": "dist/cjs/index.js",
  "types": "dist/types/index.d.ts",
  "files": [
    "dist/"
  ],
  "keywords": [
    "webpack",
    "react"
  ],
  "scripts": {
    "build": "npm run clean && npm run build:cjs && npm run copy",
    "build:cjs": "tsc --project ./tsconfig.cjs.json",
    "build:umd": "NODE_ENV=production webpack --config ./scripts/webpack/webpack.config.umd.js",
    "clean": "rimraf dist",
    "copy": "copyfiles -u 1 './src/**/*.{css,svg,less}' dist/cjs/",
    "dev": "webpack-dev-server --config ./scripts/webpack/webpack.config.dev.js --hot",
    "pub": "npm run build && npm publish",
    "start": "npm run dev"
  },
  "dependencies": {
    "@m-fe/utils": "^0.4.3",
    "eventemitter3": "^3.1.0",
    "fc-hotkeys": "^0.0.1-alpha.1",
    "interactjs": "^1.4.9",
    "lodash.debounce": "^4.0.8",
    "prop-types": "^15.6.2",
    "react": "^16.8.0",
    "react-dom": "^16.8.0",
    "siema": "^1.5.1",
    "tippy.js": "^4.3.4",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "@types/uuid": "^8.3.0",
    "copyfiles": "^2.1.0",
    "core-js": "^3.7.0",
    "prettier-plugin-pkg": "^0.8.0",
    "webpack": "^4.28.3"
  },
  "lint-staged": {
    "*.{ts,tsx,scss,less,md}": [
      "prettier --write",
      "git add"
    ]
  }
}
